    <title>Edit</title>
</head>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.js"></script>
<body>

    <script>
        $(document).ready(function () {
            $.ajax({
                type: "PUT",
                url: "http://ndemirovic.app.fit.ba/api/Usluge/13",
                data: {
                    UslugaID = 13,
                    NazivUsluge = "Šišanje",
                    TrajanjeMinuta = 30,
